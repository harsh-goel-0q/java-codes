package learningjava;
import java.util.*;

public class arrays {

	public static void main(String[] args) {
		String[][] cars= {{"buggati","honda"},{"hurray"}};
		System.out.println(cars[1].length);
		String name="Harsh";
		System.out.println(name.length());
		System.out.println(name.indexOf("ar"));
		ArrayList<String> food=new ArrayList();
		food.add("kimchi");
		food.add("pork belly");
		food.add("achar");
		ArrayList<String> drink=new ArrayList();
		drink.add("water");
		drink.add("milk");
		drink.add("coffee");
		ArrayList<ArrayList<String>> hoho= new ArrayList();
		hoho.add(food);
		hoho.add(drink);
		System.out.println(hoho);
		
	}

}
